    "markdown-it": "^11.0.0",
    "monaco-editor-webpack-plugin": "^1.9.0",
    "mousetrap": "^1.6.5",
    "quasar": "^1.0.0",
    "store": "^2.0.12",
    "sweetalert2": "^9.17.2",
    "vue-monaco": "^1.2.1",
    "webmidi": "^2.5.1"
  },
  "devDependencies": {
